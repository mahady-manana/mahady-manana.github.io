{
  "name": "next-gen-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build:export": "next build && next export",
    "deploy": "yarn build:export && yarn git",
    "git": "touch out/.nojekyll && git add . && git commit -m 'deploy site' && yarn force-subtree",
    "force-subtree": "git push origin `git subtree split --prefix out static-site`:static-site --force"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "next": "10.0.9",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "typescript": "^4.2.3"
  }
}
